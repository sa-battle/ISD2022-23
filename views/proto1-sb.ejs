<html>
  <head>
    <title>Bike Pump Finder</title>
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="style-sb.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  </head>
  <body>
    <d class="grid-container">
      <header>
        Bike Pump Finder
        <span id="search">
        <input  placeholder="Search...">
        <i class="fa fa-search"></i>
        </span>
      </header>
      <nav>
        <button class="w3-block w3-blue w3-border w3-border-white">Find bike pumps</button>
        <button class="w3-block w3-blue w3-border w3-border-white">Map</button>
      </nav>
      <main>
        <table>
          <tr>
            <th>NAME</th>
            <th>TYPE</th>
            <th>MAINTAINED</th>
          </tr>
          <% for (var i=0; i<rows.length; i++) { %>
          <tr>
            <td><%= rows[i].NAME %></td>
            <td><%= rows[i].TYPE %></td>
            <td><%= rows[i].MAINTAINED %></td>
          </tr>
          <% } %>
        </table>
      </main>
    </div>
  </body>
</html>